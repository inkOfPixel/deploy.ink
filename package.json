{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles.css",
    "dev": "concurrently -p \"[{name}]\" -n \"remix,workers,tailwind\" -c \"blue.bold,magenta.bold,green.bold\" \"npm:dev:remix\" \"npm:dev:workers\" \"npm:dev:tailwind\"",
    "dev:remix": "remix dev",
    "dev:tailwind": "tailwindcss -o ./app/styles.css --watch",
    "dev:workers": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register workers/index.ts",
    "postinstall": "remix setup node",
    "start": "npm run start:remix & npm run start:workers",
    "start:remix": "remix-serve build",
    "start:workers": "cross-env NODE_ENV=production ts-node --transpile-only -r tsconfig-paths/register workers/index.ts",
    "deploy": "./deploy.sh"
  },
  "dependencies": {
    "@remix-run/react": "1.1.1",
    "@remix-run/serve": "1.1.1",
    "bullmq": "1.59.2",
    "get-port": "6.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remix": "1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.1.1",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "concurrently": "6.4.0",
    "cross-env": "7.0.3",
    "tailwindcss": "3.0.2",
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
